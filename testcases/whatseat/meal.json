{
	"title": "API Meals",
	"api": [
		{
			"des": "Add meal",
			"method": "PUT",
			"url": "${env.url}:${env.port}/whatseat/addMeal",
			"body": {
		    "aid": "${currentUser.baseClass[0].aid}", 
		    "token": "${currentUser.baseClass[0].token}",
		    "meal": {
		    	"cuisineId": "${listCuisines.baseClass[0].itemId}",
		    	"enableReview": 0,
					"itemDescription": "Meal description",
					"photos": [
						"http://meal-photo1.jpg",
						"http://meal-photo2.jpg"
					],
					"title": "Meal title",
					"place": "${newPlace.baseClass[0]}"
		    }
			},
			"var": "newMeal",
			"expect": {
				"data": {
					"code": "OK",
					"baseClass": [
						{
							"itemId":"$(string)",
							"aid":"$(string)",
							"userAvatar":"$(string)",
							"userName":"$(string)",
							"cuisineId":"${listCuisines.baseClass[0].itemId}",
							"enableReview":0,
							"itemDescription":"Meal description",
							"place":{
								"itemId":"$(string)",
								"address":"$(string)",
								"location": "$(^[\\d\\.]+?\\s?,\\s?[\\d\\.]+$)",
								"title":"$(string)"
							},
							"nationality":"$(string)",
							"photos":"$(array)",
							"title":"$(string)",
							"ratingScore":"$(number)",
							"reviewScore":"$(number)",
							"createDate":"$(string)",
							"expiredDate":"$(string)",
							"removeDate":"$(string)",
							"state":"$(number)"
						}
					]
				}
			},
			"doc": {
				"body": "requestMeal",
				"actual": "res|responseMeals"
			}
		},
