{
	"title": "API Login",
	"api": [
		{
			"des": "Login",
			"method": "POST",
			"url": "http://localhost:8080/nanacloset/account/login",
			"body": {
			    "userName": "user@gmail.com",
			    "passWord": "12345"
			},
			"var": "currentUser",
			"expect": {
				"status": 200,
				"data": {
					"code": "OK"
				}
			}
		},
		{
			"des": "Get all campaign",
			"method": "POST",
			"url" : "http://localhost:8080/nanacloset/campaign/getListCampaign",
			"var": "listCampaign",
			"body": {
		    "aid": "${currentUser.baseClass[0].aid}",
		    "token": "${currentUser.baseClass[0].token}"
			},
			"expect": {
				"data": {
					"code": "OK"
				}	
			}
		},
		{
			"des": "Get details campaign",
			"method": "POST",
			"url": "http://localhost:8080/nanacloset/campaign/getCampaign",
			"body": {
		    "accountAid": "${currentUser.baseClass[0].aid}",
		    "campaignId": "${listCampaign.baseClass[0].id}"
			},
			"expect": {
				"status": 200,
				"data": {}
			}
		}
	]
}