{
	"title": "API Login",
	"api": [
		{
			"des": "Login",
			"method": "POST",
			"url": "${env.url}:${env.port}/hugelist/account/login",
			"body": {
			    "userName": "${env.login.username}",
			    "passWord": "${env.login.password}"
			},
			"var": "currentUser",
			"expect": {
				"status": 200,
				"data": {
					"code": "OK"
				}
			}
		}
	]
}